# no return type and without parameters:
class A
{
int l=10,b=20;
void display()
{
System.out.println(l); System.out.println(b);
}
}
class methoddemo
{
public static void main(String args[])
{
A a1=new A();
a1.display();
}
}
#output 
10
20


# no return type and with parameters:
class A
{
void display(int l,int b)
{
System.out.println(l); System.out.println(b);
}
}
class methoddemo
{
public static void main(String args[])
{
A a1=new A();
a1.display(10,20);
}
}
#OUT-PUT:
10
20
